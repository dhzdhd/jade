{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 70,
	"semi": true,
	"bracketSpacing": true,
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss"
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}